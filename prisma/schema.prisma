generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  id          Int      @id @default(autoincrement())
  createddate DateTime @db.Timestamp(6)
  email       String
  username    String
  firstname   String
  lastname    String
  phonenumber String
  password    String
  seller      Boolean
}

